# Supabase (client-side — VITE_ prefix exposes to browser)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Supabase (server-side — used by Vercel Serverless Functions only)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Google Gemini API (server-side only)
GOOGLE_GENERATIVE_AI_API_KEY=your-gemini-api-key-here

# LLM usage limits (optional — defaults shown)
MONTHLY_REQUEST_LIMIT=300
DAILY_REQUEST_LIMIT=250
